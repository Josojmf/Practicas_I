{
  "compilerOptions": {
    "typeRoots": [
        "./node_modules/@types", 
        "src/helper/types",
         "./definitions"
        ],
        "resolveJsonModule": true,
        "target": "es6",
        "module": "CommonJS",
        "outDir": "out",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}
